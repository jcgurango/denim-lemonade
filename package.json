{
  "name": "myapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "web": "env SKIP_PREFLIGHT_CHECK=true craco start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "server": "ts-node --project src/server/tsconfig.json src/server/index.ts",
    "server:dev": "nodemon --exec \"npx ts-node --project src/server/tsconfig.json src/server/index.ts\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@overgear/yup-ast": "^1.0.3",
    "@react-native-community/checkbox": "^0.5.6",
    "@react-native-picker/picker": "^1.9.3",
    "@types/express": "^4.17.9",
    "airtable": "^0.10.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "react": "16.13.1",
    "react-dom": "^17.0.1",
    "react-native": "0.63.3",
    "react-native-web": "^0.14.8",
    "ts-node": "^9.0.0",
    "use-screen-size": "^1.1.0",
    "yup": "^0.29.3",
    "yup-ast": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.56",
    "@types/react-native": "^0.63.33",
    "@types/react-test-renderer": "^16.9.2",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "craco": "^0.0.3",
    "craco-babel-loader": "^0.1.4",
    "metro-react-native-babel-preset": "^0.59.0",
    "nodemon": "^2.0.6",
    "patch-package": "^6.2.2",
    "react-scripts": "^4.0.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "airtable-schema-generator": {
    "mode": "Auto",
    "output": "src/schema"
  }
}
